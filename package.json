{
  "name": "@shipengine/integration-platform-loader",
  "version": "0.0.14",
  "description": "Internal library for loading ShipEngine Integration Platform apps",
  "keywords": [
    "shipengine",
    "integration",
    "ipaas",
    "app"
  ],
  "author": {
    "name": "ShipEngine",
    "email": "support@shipengine.com",
    "url": "https://www.shipengine.com"
  },
  "license": "Apache-2.0",
  "homepage": "https://www.shipengine.com/docs/integration-platform/",
  "repository": {
    "type": "git",
    "url": "https://github.com/ShipEngine/shipengine-integration-platform-loader.git"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "clean": "shx rm -rf .nyc_output coverage lib",
    "lint": "npm run lint:typescript && npm run lint:javascript",
    "lint:typescript": "tslint -p tsconfig.json",
    "lint:javascript": "eslint test",
    "build": "tsc",
    "watch": "tsc --watch",
    "test": "mocha && npm run lint",
    "coverage": "nyc node_modules/mocha/bin/mocha",
    "upgrade": "npm-check -u && npm audit fix",
    "bump": "bump patch --tag --push --all",
    "release": "npm run upgrade && npm run clean && npm run build && npm test && npm run bump"
  },
  "engines": {
    "node": ">=10"
  },
  "engineStrict": true,
  "devDependencies": {
    "@jsdevtools/eslint-config-modular": "^8.0.3",
    "@jsdevtools/tslint-modular": "^2.0.5",
    "@jsdevtools/version-bump-prompt": "^6.0.2",
    "@types/chai": "^4.1.7",
    "@types/hapi__joi": "^17.1.0",
    "@types/js-yaml": "^3.12.4",
    "@types/json5": "0.0.30",
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.0.1",
    "@types/react": "^16.9.35",
    "@types/source-map-support": "^0.5.1",
    "chai": "^4.2.0",
    "eslint": "^7.1.0",
    "json-schema": "^0.2.5",
    "mocha": "^7.2.0",
    "npm-check": "^5.9.0",
    "nyc": "^15.0.1",
    "react-jsonschema-form": "^1.8.1",
    "shx": "^0.3.2",
    "tslint": "^6.1.1",
    "typescript-tslint-plugin": "^0.5.0"
  },
  "dependencies": {
    "@shipengine/integration-platform-sdk": "0.0.17",
    "js-yaml": "^3.14.0",
    "json5": "^2.1.3",
    "resolve-from": "^5.0.0",
    "source-map-support": "^0.5.16",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.2"
  }
}
